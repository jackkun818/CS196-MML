<Window x:Class="crs.game.Games.EvaluateTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        d:Background="White"
        d:Height="1080"
        d:Width="1920"
        mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.2*" />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition Width="0.2*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*" />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition Height="0.2*" />
        </Grid.RowDefinitions>
        <Button x:Name="Button_1"
                Grid.Row="1"
                Grid.Column="1"
                Margin="20"
                Click="Button_Click_1"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_2"
                Grid.Row="1"
                Grid.Column="2"
                Margin="20"
                Click="Button_Click_2"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_3"
                Grid.Row="2"
                Grid.Column="1"
                Margin="20"
                Click="Button_Click_3"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_4"
                Grid.Row="2"
                Grid.Column="2"
                Margin="20"
                Click="Button_Click_4"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_5"
                Grid.Row="3"
                Grid.Column="1"
                Margin="20"
                Click="Button_Click_5"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_6"
                Grid.Row="3"
                Grid.Column="2"
                Margin="20"
                Click="Button_Click_6"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_7"
                Grid.Row="4"
                Grid.Column="1"
                Margin="20"
                Click="Button_Click_7"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
        <Button x:Name="Button_8"
                Grid.Row="4"
                Grid.Column="2"
                Margin="20"
                Click="Button_Click_8"
                Content=""
                FontSize="40"
                Style="{x:Null}" />
    </Grid>
</Window>
